<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fuckmyclassic.controller.MainWindow">
   <children>
      <MenuBar maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
        <menus>
          <Menu mnemonicParsing="false" text="%MainWindow.mnuFile">
            <items>
              <MenuItem mnemonicParsing="false" text="%MainWindow.mnuClose" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%MainWindow.mnuKernel">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onMembootCustomKernelClicked" text="%MainWindow.mnuMembootCustomKernel" />
              <MenuItem mnemonicParsing="false" onAction="#onFlashCustomKernelClicked" text="%MainWindow.mnuFlashCustomKernel" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%MainWindow.mnuHelp">
            <items>
              <MenuItem mnemonicParsing="false" text="%MainWindow.mnuAbout" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
         <children>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="%MainWindow.lblCurrentCollection" />
                  <HBox fillHeight="false" maxWidth="1.7976931348623157E308" spacing="10.0" VBox.vgrow="NEVER">
                     <children>
                        <ComboBox fx:id="cmbCurrentCollection" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                        <Button fx:id="btnFolderOptions" mnemonicParsing="false" text="%MainWindow.btnStructureOptions" />
                     </children>
                  </HBox>
                  <TreeView fx:id="treeViewGames" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin></TreeView>
                  <HBox maxWidth="1.7976931348623157E308" spacing="10.0" VBox.vgrow="NEVER">
                     <children>
                        <Button fx:id="btnAddMoreGames" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="50.0" text="%MainWindow.btnAddMoreGames" HBox.hgrow="ALWAYS" />
                        <Button fx:id="btnExportGames" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="50.0" text="%MainWindow.btnExportGames" HBox.hgrow="ALWAYS" />
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="500.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <children>
                  <TitledPane animated="false" collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%MainWindow.lblGameOptions" VBox.vgrow="ALWAYS" />
                  <Button fx:id="btnSyncGames" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="50.0" text="%MainWindow.btnSynchronizeGames">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
      <BorderPane maxWidth="1.7976931348623157E308">
         <left>
            <HBox alignment="CENTER_LEFT" fillHeight="false" BorderPane.alignment="CENTER">
               <children>
                  <Circle fx:id="shpConnectionStatus" fill="#dd0f0f" radius="10.0" stroke="BLACK" strokeType="INSIDE" />
                  <Label text="%MainWindow.lblConsoleDisconnected">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
         </left>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </VBox.margin>
         <center>
            <ProgressBar maxWidth="1.7976931348623157E308" prefWidth="500.0" progress="0.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </ProgressBar>
         </center>
         <right>
            <Label text="%MainWindow.lblNumGameSelected" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </children>
</VBox>
