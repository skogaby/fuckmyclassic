<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar VBox.vgrow="NEVER">
        <menus>
          <Menu mnemonicParsing="false" text="%MainWindow.mnuFile">
            <items>
              <MenuItem mnemonicParsing="false" text="%MainWindow.mnuClose" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%MainWindow.mnuKernel">
            <items>
              <MenuItem mnemonicParsing="false" text="%MainWindow.mnuMembootCustomKernel" />
                  <MenuItem mnemonicParsing="false" text="%MainWindow.mnuFlashCustomKernel" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%MainWindow.mnuHelp">
            <items>
              <MenuItem mnemonicParsing="false" text="%MainWindow.mnuAbout" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="%MainWindow.lblCurrentCollection" />
                  <HBox maxWidth="1.7976931348623157E308" spacing="10.0" VBox.vgrow="NEVER">
                     <children>
                        <ComboBox fx:id="currentGamesCollection" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" text="%MainWindow.btnStructureOptions" />
                     </children>
                  </HBox>
                  <TreeView fx:id="treeViewGames" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin></TreeView>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </padding>
            </VBox>
            <TitledPane animated="false" collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%MainWindow.lblGameOptions" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </TitledPane>
         </children>
      </HBox>
   </children>
</VBox>
